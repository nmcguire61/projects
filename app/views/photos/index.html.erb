<h1>Index page</h1>
<table>
  <thead>
      <th>Name</th>
      <th colspan="3"></th>
  </thead>
  <tbody>
    <% @photos.each do |photo|  %>
      <tr>
        <td><%= photo.name %></td>
        <td><%= image_tag photo.photo_image.medium.url %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class='bottomish'>
  <%= link_to raw('New Photo'), new_photo_path %>
</div>