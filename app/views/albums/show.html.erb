<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @album.name %>
</p>

<tbody>

  <% @photos.each do |photo|  %>
  
    
    <tr>
      <td><%= photo.name %></td>
      <td><%= image_tag photo.photo_image.medium.url %></td>
      <td><%= link_to 'Show', photo %></td>
      <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
      <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td><br>
    </tr>
    <% end %>
  
</tbody>

<%= link_to 'Edit', edit_album_path(@album) %> |
<%= link_to 'Back', albums_path %>